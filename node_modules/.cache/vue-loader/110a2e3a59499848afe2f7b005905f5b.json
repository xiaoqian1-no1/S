;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA","file":"BigScreen3D.vue","sourceRoot":"src/views","sourcesContent":["<template>\r\n  <div class=\"big-screen-container\">\r\n    <!-- È°∂ÈÉ®‰ø°ÊÅØÊ†è -->\r\n    <div class=\"top-info-bar\">\r\n      <div class=\"title\">\r\n        <i class=\"el-icon-s-data\"></i>\r\n        Êô∫ÊÖßÂÜúÂú∫ÂèØËßÜÂåñÂ§ßÂ±è\r\n      </div>\r\n      <div class=\"info-item\">\r\n        <i class=\"el-icon-time\"></i>\r\n        <span>{{ currentTime }}</span>\r\n      </div>\r\n      <div class=\"info-item\">\r\n        <i class=\"el-icon-sunny\"></i>\r\n        <span>{{ weather.temp }}¬∞C {{ weather.status }}</span>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- ‰∏ªÂÜÖÂÆπÂå∫ -->\r\n    <div class=\"main-content\">\r\n      <!-- Â∑¶‰æßÔºöÁªüËÆ°Ê¶ÇËßà -->\r\n      <div class=\"left-section\">\r\n        <div class=\"stat-card-group\">\r\n          <div class=\"stat-card\">\r\n            <div class=\"stat-icon\" style=\"background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);\">\r\n              <i class=\"el-icon-s-grid\"></i>\r\n            </div>\r\n            <div class=\"stat-content\">\r\n              <div class=\"stat-label\">ÂÜúÁî∞ÊÄªÊï∞</div>\r\n              <div class=\"stat-value\">{{ summary.farmCount }}<span class=\"unit\">Âùó</span></div>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"stat-card\">\r\n            <div class=\"stat-icon\" style=\"background: linear-gradient(135deg, #f093fb 0%, #f5576c 100%);\">\r\n              <i class=\"el-icon-s-data\"></i>\r\n            </div>\r\n            <div class=\"stat-content\">\r\n              <div class=\"stat-label\">ÊÄªÈù¢ÁßØ</div>\r\n              <div class=\"stat-value\">{{ summary.totalArea }}<span class=\"unit\">‰∫©</span></div>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"stat-card\">\r\n            <div class=\"stat-icon\" style=\"background: linear-gradient(135deg, #4facfe 0%, #00f2fe 100%);\">\r\n              <i class=\"el-icon-s-goods\"></i>\r\n            </div>\r\n            <div class=\"stat-content\">\r\n              <div class=\"stat-label\">‰ΩúÁâ©ÊÄªÈáè</div>\r\n              <div class=\"stat-value\">{{ summary.totalStock }}<span class=\"unit\">Ê†™</span></div>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"stat-card\">\r\n            <div class=\"stat-icon\" style=\"background: linear-gradient(135deg, #43e97b 0%, #38f9d7 100%);\">\r\n              <i class=\"el-icon-success\"></i>\r\n            </div>\r\n            <div class=\"stat-content\">\r\n              <div class=\"stat-label\">Ê≠£Â∏∏ËøêË°å</div>\r\n              <div class=\"stat-value\">{{ summary.normalCount }}<span class=\"unit\">Âùó</span></div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- ÂÜúÁî∞ÂàóË°® -->\r\n        <div class=\"farm-list-panel\">\r\n          <div class=\"panel-title\">\r\n            <i class=\"el-icon-menu\"></i>\r\n            ÂÜúÁî∞ÂàóË°®\r\n          </div>\r\n          <div class=\"farm-list\">\r\n            <div \r\n              v-for=\"(farm, index) in farms\" \r\n              :key=\"index\"\r\n              class=\"farm-item\"\r\n              :class=\"{ active: selectedFarm && selectedFarm.id === farm.id }\"\r\n              @click=\"selectFarm(farm)\"\r\n            >\r\n              <div class=\"farm-header\">\r\n                <span class=\"farm-name\">{{ farm.farm }}</span>\r\n                <el-tag :type=\"farm.state === 'Ê≠£Â∏∏' ? 'success' : 'warning'\" size=\"mini\">\r\n                  {{ farm.state }}\r\n                </el-tag>\r\n              </div>\r\n              <div class=\"farm-info\">\r\n                <div class=\"info-item\">\r\n                  <i class=\"el-icon-location\"></i>\r\n                  {{ farm.address }}\r\n                </div>\r\n                <div class=\"info-row\">\r\n                  <span>üåæ {{ farm.crop }}</span>\r\n                  <span>üìè {{ farm.area }}‰∫©</span>\r\n                  <span>üå± {{ farm.number }}Ê†™</span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- ‰∏≠Èó¥Ôºö3D ÂèØËßÜÂåñ -->\r\n      <div class=\"center-section\">\r\n        <div class=\"panel-title\">\r\n          <i class=\"el-icon-view\"></i>\r\n          ÂÜúÁî∞ 3D ÂèØËßÜÂåñ\r\n        </div>\r\n        <div class=\"chart-3d\" ref=\"chart3d\"></div>\r\n      </div>\r\n\r\n      <!-- Âè≥‰æßÔºöËØ¶ÁªÜ‰ø°ÊÅØ -->\r\n      <div class=\"right-section\">\r\n        <!-- ÈÄâ‰∏≠ÂÜúÁî∞ËØ¶ÊÉÖ -->\r\n        <div class=\"detail-panel\">\r\n          <div class=\"panel-title\">\r\n            <i class=\"el-icon-document\"></i>\r\n            ÂÜúÁî∞ËØ¶ÊÉÖ\r\n          </div>\r\n          <div v-if=\"selectedFarm\" class=\"detail-content\">\r\n            <div class=\"detail-header\">\r\n              <h3>{{ selectedFarm.farm }}</h3>\r\n              <el-tag :type=\"selectedFarm.state === 'Ê≠£Â∏∏' ? 'success' : 'warning'\">\r\n                {{ selectedFarm.state }}\r\n              </el-tag>\r\n            </div>\r\n            \r\n            <div class=\"detail-grid\">\r\n              <div class=\"detail-item\">\r\n                <div class=\"detail-label\">‰ΩúÁâ©Á±ªÂûã</div>\r\n                <div class=\"detail-value\">{{ selectedFarm.crop }}</div>\r\n              </div>\r\n              <div class=\"detail-item\">\r\n                <div class=\"detail-label\">ÁßçÊ§çÈù¢ÁßØ</div>\r\n                <div class=\"detail-value\">{{ selectedFarm.area }} ‰∫©</div>\r\n              </div>\r\n              <div class=\"detail-item\">\r\n                <div class=\"detail-label\">‰ΩúÁâ©Êï∞Èáè</div>\r\n                <div class=\"detail-value\">{{ selectedFarm.number }} Ê†™</div>\r\n              </div>\r\n              <div class=\"detail-item\">\r\n                <div class=\"detail-label\">ÊâÄÂú®‰ΩçÁΩÆ</div>\r\n                <div class=\"detail-value\">{{ selectedFarm.address }}</div>\r\n              </div>\r\n              <div class=\"detail-item\">\r\n                <div class=\"detail-label\">Ë¥üË¥£‰∫∫</div>\r\n                <div class=\"detail-value\">{{ selectedFarm.keeper }}</div>\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"env-monitor\">\r\n              <div class=\"monitor-title\">ÁéØÂ¢ÉÁõëÊµãÊï∞ÊçÆ</div>\r\n              <el-row :gutter=\"10\">\r\n                <el-col :span=\"12\">\r\n                  <div class=\"monitor-item\">\r\n                    <i class=\"el-icon-sunny\" style=\"color: #E6A23C;\"></i>\r\n                    <span class=\"monitor-label\">Ê∏©Â∫¶</span>\r\n                    <span class=\"monitor-value\">{{ selectedFarm.temperature }}¬∞C</span>\r\n                  </div>\r\n                </el-col>\r\n                <el-col :span=\"12\">\r\n                  <div class=\"monitor-item\">\r\n                    <i class=\"el-icon-water-cup\" style=\"color: #409EFF;\"></i>\r\n                    <span class=\"monitor-label\">Á©∫Ê∞îÊπøÂ∫¶</span>\r\n                    <span class=\"monitor-value\">{{ selectedFarm.airhumidity }}%</span>\r\n                  </div>\r\n                </el-col>\r\n                <el-col :span=\"12\">\r\n                  <div class=\"monitor-item\">\r\n                    <i class=\"el-icon-s-opportunity\" style=\"color: #67C23A;\"></i>\r\n                    <span class=\"monitor-label\">ÂúüÂ£§ÊπøÂ∫¶</span>\r\n                    <span class=\"monitor-value\">{{ selectedFarm.soilhumidity }}%</span>\r\n                  </div>\r\n                </el-col>\r\n                <el-col :span=\"12\">\r\n                  <div class=\"monitor-item\">\r\n                    <i class=\"el-icon-light\" style=\"color: #F56C6C;\"></i>\r\n                    <span class=\"monitor-label\">ÂÖâÁÖßÂº∫Â∫¶</span>\r\n                    <span class=\"monitor-value\">{{ selectedFarm.light }}lux</span>\r\n                  </div>\r\n                </el-col>\r\n                <el-col :span=\"12\">\r\n                  <div class=\"monitor-item\">\r\n                    <i class=\"el-icon-s-marketing\" style=\"color: #909399;\"></i>\r\n                    <span class=\"monitor-label\">CO‚ÇÇÂê´Èáè</span>\r\n                    <span class=\"monitor-value\">{{ selectedFarm.carbon }}ppm</span>\r\n                  </div>\r\n                </el-col>\r\n                <el-col :span=\"12\">\r\n                  <div class=\"monitor-item\">\r\n                    <i class=\"el-icon-s-tools\" style=\"color: #E6A23C;\"></i>\r\n                    <span class=\"monitor-label\">ÂúüÂ£§PHÂÄº</span>\r\n                    <span class=\"monitor-value\">{{ selectedFarm.ph }}</span>\r\n                  </div>\r\n                </el-col>\r\n              </el-row>\r\n            </div>\r\n\r\n            <!-- ÂÅ•Â∫∑ÊåáÊï∞ÊòæÁ§∫ -->\r\n            <div class=\"health-index-section\" v-if=\"selectedFarm\">\r\n              <div class=\"monitor-title\">ÂÅ•Â∫∑ÊåáÊï∞</div>\r\n              <div class=\"health-index-display\">\r\n                <div class=\"health-score\" :style=\"{ background: getHealthScoreGradient(selectedFarm) }\">\r\n                  <div class=\"score-value\">{{ calculateHealthScoreLocal(selectedFarm) }}</div>\r\n                  <div class=\"score-label\">ÂÅ•Â∫∑ÊåáÊï∞</div>\r\n                </div>\r\n                <div class=\"health-level\">\r\n                  <el-tag :type=\"getHealthLevelTag(selectedFarm)\" size=\"medium\">\r\n                    {{ getHealthLevelText(selectedFarm) }}\r\n                  </el-tag>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"device-status\">\r\n              <div class=\"monitor-title\">ËÆæÂ§áÁä∂ÊÄÅ</div>\r\n              <div class=\"device-list\">\r\n                <div class=\"device-item\">\r\n                  <span>üí° Ë°•ÂÖâÁÅØ</span>\r\n                  <el-tag :type=\"selectedFarm.filllight === 'ÂºÄÂêØ' ? 'success' : 'info'\" size=\"small\">\r\n                    {{ selectedFarm.filllight }}\r\n                  </el-tag>\r\n                </div>\r\n                <div class=\"device-item\">\r\n                  <span>üìπ ÊëÑÂÉèÂ§¥</span>\r\n                  <el-tag :type=\"selectedFarm.monitor === 'ÂºÄÂêØ' ? 'success' : 'info'\" size=\"small\">\r\n                    {{ selectedFarm.monitor }}\r\n                  </el-tag>\r\n                </div>\r\n                <div class=\"device-item\">\r\n                  <span>üíß Ê∞¥Ê≥µ</span>\r\n                  <el-tag :type=\"selectedFarm.pump === 'ÂºÄÂêØ' ? 'success' : 'info'\" size=\"small\">\r\n                    {{ selectedFarm.pump }}\r\n                  </el-tag>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div v-else class=\"no-selection\">\r\n            <i class=\"el-icon-info\"></i>\r\n            <p>ËØ∑ÁÇπÂáªÂ∑¶‰æßÂÜúÁî∞ÂàóË°®Êü•ÁúãËØ¶ÊÉÖ</p>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- ‰ªäÊó•‰ªªÂä°Ê∏ÖÂçï -->\r\n        <div class=\"task-panel\">\r\n          <TodayTaskList />\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport * as echarts from 'echarts';\r\nimport 'echarts-gl';\r\nimport TodayTaskList from '@/components/TodayTaskList.vue';\r\n\r\nexport default {\r\n  name: 'BigScreen3D',\r\n  components: {\r\n    TodayTaskList\r\n  },\r\n  data() {\r\n    return {\r\n      currentTime: '',\r\n      selectedFarm: null,\r\n      // Â§©Ê∞î‰ø°ÊÅØ\r\n      weather: {\r\n        temp: 28,\r\n        status: 'Êô¥'\r\n      },\r\n      // ÁªüËÆ°Êï∞ÊçÆ\r\n      summary: {\r\n        farmCount: 0,\r\n        totalArea: 0,\r\n        totalStock: 0,\r\n        normalCount: 0\r\n      },\r\n      // ÂÜúÁî∞Êï∞ÊçÆ\r\n      farms: []\r\n    };\r\n  },\r\n  mounted() {\r\n    this.updateTime();\r\n    setInterval(this.updateTime, 1000);\r\n    this.fetchData();\r\n  },\r\n  methods: {\r\n    updateTime() {\r\n      const now = new Date();\r\n      const year = now.getFullYear();\r\n      const month = String(now.getMonth() + 1).padStart(2, '0');\r\n      const day = String(now.getDate()).padStart(2, '0');\r\n      const hour = String(now.getHours()).padStart(2, '0');\r\n      const minute = String(now.getMinutes()).padStart(2, '0');\r\n      const second = String(now.getSeconds()).padStart(2, '0');\r\n      const weekDays = ['ÊòüÊúüÊó•', 'ÊòüÊúü‰∏Ä', 'ÊòüÊúü‰∫å', 'ÊòüÊúü‰∏â', 'ÊòüÊúüÂõõ', 'ÊòüÊúü‰∫î', 'ÊòüÊúüÂÖ≠'];\r\n      const weekDay = weekDays[now.getDay()];\r\n      \r\n      this.currentTime = `${year}-${month}-${day} ${hour}:${minute}:${second} ${weekDay}`;\r\n    },\r\n    \r\n    async fetchData() {\r\n      try {\r\n        const farmsRes = await this.request.get('/statistic/dashboard');\r\n        if (farmsRes.code === '200') {\r\n          this.farms = farmsRes.data;\r\n          if (this.farms.length > 0) {\r\n            this.selectedFarm = this.farms[0];\r\n          }\r\n          \r\n          // Êï∞ÊçÆÂä†ËΩΩÂÆåÊàêÂêéÂàùÂßãÂåñ3DÂõæË°®\r\n          this.$nextTick(() => {\r\n            this.init3DChart();\r\n          });\r\n        }\r\n        \r\n        const summaryRes = await this.request.get('/statistic/dashboard/summary');\r\n        if (summaryRes.code === '200') {\r\n          this.summary = summaryRes.data;\r\n        }\r\n      } catch (error) {\r\n        console.error('Ëé∑ÂèñÊï∞ÊçÆÂ§±Ë¥•:', error);\r\n      }\r\n    },\r\n    \r\n    selectFarm(farm) {\r\n      this.selectedFarm = farm;\r\n    },\r\n    \r\n    init3DChart() {\r\n      if (!this.$refs.chart3d) {\r\n        console.error('ÂõæË°®ÂÆπÂô®Êú™ÊâæÂà∞');\r\n        return;\r\n      }\r\n      \r\n      if (!this.farms || this.farms.length === 0) {\r\n        console.warn('Ê≤°ÊúâÂÜúÁî∞Êï∞ÊçÆ');\r\n        return;\r\n      }\r\n      \r\n      console.log('ÂàùÂßãÂåñ3DÂõæË°®ÔºåÂÜúÁî∞Êï∞Èáè:', this.farms.length);\r\n      \r\n      const chart = echarts.init(this.$refs.chart3d);\r\n      \r\n      // ÂáÜÂ§á3DÊü±Áä∂ÂõæÊï∞ÊçÆ - ÊØè‰∏™ÂÜúÁî∞‰∏Ä‰∏™Êü±Â≠ê\r\n      const barData = [];\r\n      this.farms.forEach((farm, index) => {\r\n        const x = index % 5;\r\n        const y = Math.floor(index / 5);\r\n        const z = parseFloat(farm.area) || 5;\r\n        \r\n        barData.push({\r\n          value: [x, y, z],\r\n          itemStyle: {\r\n            color: this.getCropColor(farm.crop)\r\n          },\r\n          label: {\r\n            show: true,\r\n            formatter: farm.farm + '\\n' + farm.area + '‰∫©\\n' + farm.crop\r\n          },\r\n          farmData: farm\r\n        });\r\n      });\r\n      \r\n      console.log('3DÊü±Áä∂ÂõæÊï∞ÊçÆ:', barData);\r\n      \r\n      const option = {\r\n        tooltip: {\r\n          formatter: function(params) {\r\n            if (params.data && params.data.farmData) {\r\n              const f = params.data.farmData;\r\n              return f.farm + '<br/>Èù¢ÁßØ: ' + f.area + '‰∫©<br/>‰ΩúÁâ©: ' + f.crop + '<br/>Êï∞Èáè: ' + f.number + 'Ê†™';\r\n            }\r\n            return '';\r\n          }\r\n        },\r\n        xAxis3D: {\r\n          type: 'value'\r\n        },\r\n        yAxis3D: {\r\n          type: 'value'\r\n        },\r\n        zAxis3D: {\r\n          type: 'value',\r\n          name: 'Èù¢ÁßØ(‰∫©)'\r\n        },\r\n        grid3D: {\r\n          boxWidth: 100,\r\n          boxDepth: 80,\r\n          viewControl: {\r\n            autoRotate: true,\r\n            autoRotateSpeed: 10\r\n          }\r\n        },\r\n        series: [{\r\n          type: 'bar3D',\r\n          data: barData,\r\n          shading: 'lambert',\r\n          label: {\r\n            textStyle: {\r\n              fontSize: 12,\r\n              borderWidth: 1,\r\n              borderColor: '#fff',\r\n              backgroundColor: 'rgba(0,0,0,0.6)',\r\n              color: '#fff',\r\n              padding: 3\r\n            }\r\n          },\r\n          itemStyle: {\r\n            opacity: 0.8\r\n          },\r\n          emphasis: {\r\n            label: {\r\n              textStyle: {\r\n                fontSize: 14,\r\n                backgroundColor: 'rgba(64,158,255,0.8)'\r\n              }\r\n            },\r\n            itemStyle: {\r\n              opacity: 1\r\n            }\r\n          }\r\n        }]\r\n      };\r\n      \r\n      chart.setOption(option);\r\n      console.log('3DÂõæË°®ÈÖçÁΩÆÂÆåÊàê');\r\n      \r\n      // ÁÇπÂáª‰∫ã‰ª∂\r\n      chart.on('click', (params) => {\r\n        if (params.data && params.data.farmData) {\r\n          this.selectFarm(params.data.farmData);\r\n        }\r\n      });\r\n      \r\n      window.addEventListener('resize', () => {\r\n        chart.resize();\r\n      });\r\n    },\r\n    \r\n    getCropColor(crop) {\r\n      const colors = {\r\n        'Â∞èÈ∫¶': '#F4D03F',\r\n        'ÁéâÁ±≥': '#F39C12',\r\n        'Ê∞¥Á®ª': '#52BE80',\r\n        'Â§ßË±Ü': '#85C1E2',\r\n        'Áï™ËåÑ': '#E74C3C',\r\n        'ÈªÑÁìú': '#27AE60',\r\n        'Ëæ£Ê§í': '#C0392B',\r\n        'ËåÑÂ≠ê': '#8E44AD',\r\n        'ÁôΩËèú': '#2ECC71',\r\n        'ËêùÂçú': '#E67E22',\r\n        'ÂúüË±Ü': '#D4AC6E',\r\n        'ÂçóÁìú': '#FF8C00'\r\n      };\r\n      return colors[crop] || '#409EFF';\r\n    },\r\n    \r\n    // ËÆ°ÁÆóÂÅ•Â∫∑ÊåáÊï∞\r\n    async calculateHealthScore(farm) {\r\n      if (!farm || !farm.id) return 0;\r\n      try {\r\n        const res = await this.request.get(`/health-index/calculate/${farm.id}`);\r\n        if (res.code === '200' && res.data) {\r\n          return res.data.healthIndex || 0;\r\n        }\r\n      } catch (error) {\r\n        console.error('ËÆ°ÁÆóÂÅ•Â∫∑ÊåáÊï∞Â§±Ë¥•:', error);\r\n      }\r\n      // Â¶ÇÊûúAPIÂ§±Ë¥•Ôºå‰ΩøÁî®Êú¨Âú∞ËÆ°ÁÆó\r\n      return this.calculateHealthScoreLocal(farm);\r\n    },\r\n    \r\n    // Êú¨Âú∞ËÆ°ÁÆóÂÅ•Â∫∑ÊåáÊï∞ÔºàÂ§áÁî®ÊñπÊ≥ïÔºâ\r\n    calculateHealthScoreLocal(farm) {\r\n      if (!farm) return 0;\r\n      let totalScore = 0;\r\n      let count = 0;\r\n      \r\n      // Ê∏©Â∫¶ËØÑÂàÜ\r\n      if (farm.temperature != null) {\r\n        const temp = parseFloat(farm.temperature);\r\n        if (temp >= 20 && temp <= 28) totalScore += 100;\r\n        else if (temp >= 15 && temp <= 32) totalScore += 75;\r\n        else totalScore += 50;\r\n        count++;\r\n      }\r\n      \r\n      // Á©∫Ê∞îÊπøÂ∫¶ËØÑÂàÜ\r\n      if (farm.airhumidity != null) {\r\n        const airHum = parseInt(farm.airhumidity);\r\n        if (airHum >= 60 && airHum <= 80) totalScore += 100;\r\n        else if (airHum >= 50 && airHum <= 90) totalScore += 75;\r\n        else totalScore += 50;\r\n        count++;\r\n      }\r\n      \r\n      // ÂúüÂ£§ÊπøÂ∫¶ËØÑÂàÜ\r\n      if (farm.soilhumidity != null) {\r\n        const soilHum = parseInt(farm.soilhumidity);\r\n        if (soilHum >= 40 && soilHum <= 70) totalScore += 100;\r\n        else if (soilHum >= 30 && soilHum <= 80) totalScore += 75;\r\n        else totalScore += 50;\r\n        count++;\r\n      }\r\n      \r\n      // CO2ËØÑÂàÜ\r\n      if (farm.carbon != null) {\r\n        const carbon = parseInt(farm.carbon);\r\n        if (carbon >= 350 && carbon <= 450) totalScore += 100;\r\n        else if (carbon >= 300 && carbon <= 1500) totalScore += 75;\r\n        else totalScore += 50;\r\n        count++;\r\n      }\r\n      \r\n      // pHËØÑÂàÜ\r\n      if (farm.ph != null) {\r\n        const ph = parseFloat(farm.ph);\r\n        if (ph >= 6.0 && ph <= 7.5) totalScore += 100;\r\n        else if (ph >= 5.5 && ph <= 8.0) totalScore += 75;\r\n        else totalScore += 50;\r\n        count++;\r\n      }\r\n      \r\n      // ÂÖâÁÖßËØÑÂàÜ\r\n      if (farm.light != null) {\r\n        const light = parseInt(farm.light);\r\n        if (light >= 2000 && light <= 3000) totalScore += 100;\r\n        else if (light >= 1500 && light <= 3500) totalScore += 75;\r\n        else totalScore += 50;\r\n        count++;\r\n      }\r\n      \r\n      if (count === 0) return 0;\r\n      return Math.round(totalScore / count);\r\n    },\r\n    \r\n    // Ëé∑ÂèñÂÅ•Â∫∑ÊåáÊï∞Ê∏êÂèòËÉåÊôØ\r\n    getHealthScoreGradient(farm) {\r\n      const score = this.calculateHealthScoreLocal(farm);\r\n      if (score >= 90) {\r\n        return 'linear-gradient(135deg, #67C23A 0%, #85CE61 100%)';\r\n      } else if (score >= 75) {\r\n        return 'linear-gradient(135deg, #409EFF 0%, #66B1FF 100%)';\r\n      } else if (score >= 60) {\r\n        return 'linear-gradient(135deg, #E6A23C 0%, #EEBE77 100%)';\r\n      } else {\r\n        return 'linear-gradient(135deg, #F56C6C 0%, #F78989 100%)';\r\n      }\r\n    },\r\n    \r\n    // Ëé∑ÂèñÂÅ•Â∫∑Á≠âÁ∫ßÊ†áÁ≠æÁ±ªÂûã\r\n    getHealthLevelTag(farm) {\r\n      const score = this.calculateHealthScoreLocal(farm);\r\n      if (score >= 90) return 'success';\r\n      if (score >= 75) return 'primary';\r\n      if (score >= 60) return 'warning';\r\n      return 'danger';\r\n    },\r\n    \r\n    // Ëé∑ÂèñÂÅ•Â∫∑Á≠âÁ∫ßÊñáÊú¨\r\n    getHealthLevelText(farm) {\r\n      const score = this.calculateHealthScoreLocal(farm);\r\n      if (score >= 90) return '‰ºòÁßÄ';\r\n      if (score >= 75) return 'ËâØÂ•Ω';\r\n      if (score >= 60) return '‰∏ÄËà¨';\r\n      return 'ÈúÄÂÖ≥Ê≥®';\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.big-screen-container {\r\n 